<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="ir_cron_update_widget_action" model="ir.cron">
            <field name="name">Odoo dashboard: Update widgets</field>
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall" />
            <field eval="'reporting.dashboard.widget'" name="model" />
            <field eval="'schedule_update_widget_action'" name="function" />
        </record>
    </data>
</openerp>